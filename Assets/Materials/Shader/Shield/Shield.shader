				float temp_output_23_0 = ( IN.ase_texcoord3.xyz.y + _DissolveOffset1 );
				float4 temp_cast_0 = (temp_output_23_0).xxxx;
				float2 texCoord9 = IN.ase_texcoord4.xy * float2( 1,1 ) + float2( 0,0 );
				float4 temp_output_14_0 = ( tex2D( _Noise1, texCoord9 ) * _DissolveValue1 );
