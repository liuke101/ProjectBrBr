				float4 temp_cast_3 = (temp_output_23_0).xxxx;
				float4 temp_cast_4 = (temp_output_23_0).xxxx;
				float4 lerpResult35 = lerp( ( temp_output_30_0 + ( _BaseColor1 * ( ( pow( ( 1.0 - saturate( dotResult10 ) ) , _FresnelExp1 ) * _FresnelScale1 ) * _EdgeColor1 ) ) ) , _DissolveColor1 , temp_output_30_0);
				
				float3 BakedAlbedo = 0;
				float3 BakedEmission = 0;
				float3 Color = lerpResult35.rgb;
				float Alpha = 1;
				float AlphaClipThreshold = 0.5;
				float AlphaClipThresholdShadow = 0.5;

